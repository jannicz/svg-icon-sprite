{
  "name": "svg-icon-sprite",
  "version": "0.3.0",
  "description": "A Web Component for generating and including SVG spites",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config-name default --mode development --open",
    "build": "webpack --config-name prod",
    "generate:sprite": "node scripts/generate-sprite.js --folder=assets/freepik-icons --output=assets/sprites/sprite.svg"
  },
  "author": "Jan Suwart",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "minimist": "^1.2.0"
  }
}
